package world;
import java.util.*;

public class mod {
public static void main(String[]args) {
Scanner s= new Scanner(System.in);
System.out.println("Enter a number");
	int a = s.nextInt();
	if(a%2==0)
	{
	    System.out.println("this number is even");
	}
	else
	{
		System.out.println("this number is Odd");
	}
}
}


